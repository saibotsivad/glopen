{
  "name": "glopen",
  "version": "0.0.7",
  "description": "GLobbify OPENapi: Glob folder structures into an OpenAPI definition and API router driver.",
  "bin": "bin.js",
  "type": "module",
  "module": "dist/index.js",
  "main": "dist/index.cjs",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "test": "eslint '**/*.js' --fix",
    "prepublishOnly": "npm run build",
    "build": "bundt src/index.js"
  },
  "files": [
    "dist",
    "src",
    "bin.js",
    "README.md",
    "LICENSE.md",
    "package.json"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/saibotsivad/glopen.git"
  },
  "keywords": [
    "openapi",
    "glob",
    "generator",
    "glopen"
  ],
  "author": {
    "name": "Tobias Davis",
    "email": "tobias@davistobias.com",
    "url": "https://davistobias.com"
  },
  "license": "SEE LICENSE IN LICENSE.md",
  "bugs": {
    "url": "https://github.com/saibotsivad/glopen/issues"
  },
  "homepage": "https://github.com/saibotsivad/glopen/tree/main/package/glopen",
  "devDependencies": {
    "ajv": "^8.6.3",
    "ajv-formats": "^2.1.1",
    "ajv-openapi-compile": "^0.0.2",
    "body-parser": "^1.19.0",
    "bundt": "^1.1.5",
    "eslint": "^8.1.0",
    "polka": "^0.5.2"
  },
  "dependencies": {
    "cheap-watch": "^1.0.4",
    "mri": "^1.2.0",
    "tiny-glob": "^0.2.9",
    "to-js-identifier": "^1.0.0"
  }
}