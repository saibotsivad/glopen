#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/tobias/Development/git/glopen/node_modules/.pnpm/uvu@0.5.1/node_modules/uvu/node_modules:/Users/tobias/Development/git/glopen/node_modules/.pnpm/uvu@0.5.1/node_modules:/Users/tobias/Development/git/glopen/node_modules/.pnpm/node_modules:/Users/tobias/Development/git/glopen/node_modules:/Users/tobias/Development/git/node_modules:/Users/tobias/Development/node_modules:/Users/tobias/node_modules:/Users/node_modules:/node_modules:/Users/tobias/Development/git/glopen/integration-test/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/tobias/Development/git/glopen/node_modules/.pnpm/uvu@0.5.1/node_modules/uvu/node_modules:/Users/tobias/Development/git/glopen/node_modules/.pnpm/uvu@0.5.1/node_modules:/Users/tobias/Development/git/glopen/node_modules/.pnpm/node_modules:/Users/tobias/Development/git/glopen/node_modules:/Users/tobias/Development/git/node_modules:/Users/tobias/Development/node_modules:/Users/tobias/node_modules:/Users/node_modules:/node_modules:/Users/tobias/Development/git/glopen/integration-test/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../node_modules/.pnpm/uvu@0.5.1/node_modules/uvu/bin.js" "$@"
else
  exec node  "$basedir/../../../node_modules/.pnpm/uvu@0.5.1/node_modules/uvu/bin.js" "$@"
fi
